{
  "name": "cildef-backend",
  "version": "1.0.0",
  "description": "Node, Express, MySQL and Sequelize for CILDEF project - Abastible",
  "main": "index.js",
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "start": "npm run docs && nodemon index.js",
    "local": "nodemon index.js --exec 'npm run lint && node'",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "eslint --ignore-path .gitignore --fix .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docs": "apidoc -i ./ -e node_modules/ -t ./node_modules/apidoc-contenttype-plugin/template/ --parse-parsers apicontenttype=./node_modules/apidoc-contenttype-plugin/api_content_type.js"
  },
  "author": "SIROE SpA",
  "license": "GPL-3.0",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "apidoc": "^0.54.0",
    "apidoc-contenttype-plugin": "^0.1.0",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.18.3",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "joi": "^14.3.0",
    "joi-date-extensions": "^1.2.0",
    "jsontoxml": "^1.0.1",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^1.6.4",
    "sequelize": "^6.29.1",
    "strong-soap": "^3.5.2",
    "xml2js": "^0.6.0"
  },
  "devDependencies": {
    "eslint": "^5.10.0",
    "eslint-plugin-node": "^8.0.0",
    "nodemon": "^2.0.22"
  }
}
